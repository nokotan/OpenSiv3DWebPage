msgid "## Setting up Intellisense"
msgstr "## インテリセンスを構成する"

msgid "### Sample Configulation"
msgstr "### 参考設定"

msgid ""
"```json\n"
"{\n"
"    \"name\": \"Emscripten\",\n"
"    \"includePath\": [\n"
"        \"${workspaceFolder}/**\",\n"
"        \"${workspaceFolder}/OpenSiv3D/include\",\n"
"        \"${workspaceFolder}/OpenSiv3D/include/ThirdParty\",\n"
"        \"${env:EMSDK}/upstream/emscripten/system/include\",\n"
"        \"${env:EMSDK}/upstream/emscripten/system/include/libc\",\n"
"        \"${env:EMSDK}/upstream/emscripten/system/include/libcxx\"\n"
"    ],\n"
"    \"defines\": [\n"
"        \"_DEBUG\",\n"
"        \"EMSCRIPTEN\",\n"
"        \"__EMSCRIPTEN__\"\n"
"    ],\n"
"    \"cStandard\": \"c11\",\n"
"    \"cppStandard\": \"c++17\",\n"
"    \"intelliSenseMode\": \"clang-x86\"\n"
"}\n"
"```"
msgstr ""

msgid "## Setting up Build Task"
msgstr "## ビルドタスクを設定する"

msgid "### Compile Task Sample"
msgstr "### コンパイルタスクのサンプル"

msgid ""
"```json\n"
"{\n"
"    \"type\": \"shell\",\n"
"    \"label\": \"Build: Compile (Debug)\",\n"
"    \"command\": \"em++\",\n"
"    \"args\": [\n"
"        // Input\n"
"        \"Main.cpp\","
msgstr ""
"```json\n"
"{\n"
"    \"type\": \"shell\",\n"
"    \"label\": \"Build: Compile (Debug)\",\n"
"    \"command\": \"em++\",\n"
"    \"args\": [\n"
"        // 入力\n"
"        \"Main.cpp\","

msgid ""
"        // Output\n"
"        \"-c\", \"-o\", \"Main.o\","
msgstr ""
"        // 出力\n"
"        \"-c\", \"-o\", \"Main.o\","

msgid ""
"        // Compiler Flags\n"
"        \"-O0\", \"-std=c++17\", \"-g4\","
msgstr ""
"        // 最適化とデバッグ、言語設定\n"
"        \"-O0\", \"-std=c++17\", \"-g4\","

msgid ""
"        // Include Directory\n"
"        \"-IOpenSiv3D/include\",  \n"
"        \"-IOpenSiv3D/include/ThirdParty\","
msgstr ""
"        // インクルードディレクトリ\n"
"        \"-IOpenSiv3D/include\",  \n"
"        \"-IOpenSiv3D/include/ThirdParty\","

msgid ""
"        // em++ Specific Configurations\n"
"    ],\n"
"    \"group\": \"build\"\n"
"}\n"
"```"
msgstr ""
"        // em++ 固有の設定\n"
"    ],\n"
"    \"group\": \"build\"\n"
"}\n"
"```"

msgid "### Link Task Sample"
msgstr "### リンクタスクのサンプル"

msgid ""
"```json\n"
"{\n"
"    \"type\": \"shell\",\n"
"    \"label\": \"Build: Link (Debug)\",\n"
"    \"command\": \"em++\",\n"
"    \"args\": \n"
"    [\n"
"        // Input\n"
"        \"Main.o\","
msgstr ""
"```json\n"
"{\n"
"    \"type\": \"shell\",\n"
"    \"label\": \"Build: Link (Debug)\",\n"
"    \"command\": \"em++\",\n"
"    \"args\": \n"
"    [\n"
"        // 入力\n"
"        \"Main.o\","

msgid ""
"        // Output\n"
"        \"-o\", \"index.html\","
msgstr ""
"        // 出力\n"
"        \"-o\", \"index.html\","

msgid ""
"        // Compiler Flags\n"
"        \"-O0\", \"-std=c++17\", \"-g\","
msgstr ""
"        // 最適化とデバッグ、言語設定\n"
"        \"-O0\", \"-std=c++17\", \"-g\","

msgid ""
"        // Library Directory\n"
"        \"-LOpenSiv3D/lib\",\n"
"        \"-LOpenSiv3D/lib/freetype\",\n"
"        \"-LOpenSiv3D/lib/harfbuzz\",\n"
"        \"-LOpenSiv3D/lib/opencv\","
msgstr ""
"        // ライブラリディレクトリ\n"
"        \"-LOpenSiv3D/lib\",\n"
"        \"-LOpenSiv3D/lib/freetype\",\n"
"        \"-LOpenSiv3D/lib/harfbuzz\",\n"
"        \"-LOpenSiv3D/lib/opencv\","

msgid ""
"        // Dependent Libraries\n"
"        \"-lfreetype\",\n"
"        \"-lharfbuzz\",\n"
"        \"-lopencv_core\", \"-lopencv_imgproc\", \"-lopencv_objdetect\", \"-lopencv_p"
"hoto\",\n"
"        \"-lSiv3D\","
msgstr ""
"        // 依存ライブラリ\n"
"        \"-lfreetype\",\n"
"        \"-lharfbuzz\",\n"
"        \"-lopencv_core\", \"-lopencv_imgproc\", \"-lopencv_objdetect\", \"-lopencv_p"
"hoto\",\n"
"        \"-lSiv3D\","

msgid ""
"        // em++ Specific Configurations\n"
"        \"--emrun\",\n"
"        \"-s\", \"FULL_ES3=1\",\n"
"        \"-s\", \"USE_GLFW=3\",\n"
"        \"-s\", \"USE_LIBPNG=1\",\n"
"        \"-s\", \"USE_OGG=1\",\n"
"        \"-s\", \"USE_VORBIS=1\",\n"
"        \"-s\", \"ALLOW_MEMORY_GROWTH=1\",\n"
"        \"-s\", \"ERROR_ON_UNDEFINED_SYMBOLS=0\","
msgstr ""
"        // em++ 固有の設定\n"
"        \"--emrun\",\n"
"        \"-s\", \"FULL_ES3=1\",\n"
"        \"-s\", \"USE_GLFW=3\",\n"
"        \"-s\", \"USE_LIBPNG=1\",\n"
"        \"-s\", \"USE_OGG=1\",\n"
"        \"-s\", \"USE_VORBIS=1\",\n"
"        \"-s\", \"ALLOW_MEMORY_GROWTH=1\",\n"
"        \"-s\", \"ERROR_ON_UNDEFINED_SYMBOLS=0\","

msgid ""
"        // Packaging Configurations\n"
"        \"--preload-file\", \"OpenSiv3D/resources@/resources\",\n"
"        \"--js-library\", \"OpenSiv3D/lib/Siv3D.js\",\n"
"    ],\n"
"    \"group\": \"build\"\n"
"}\n"
"```"
msgstr ""
"        // パッケージング設定\n"
"        \"--preload-file\", \"OpenSiv3D/resources@/resources\",\n"
"        \"--js-library\", \"OpenSiv3D/lib/Siv3D.js\",\n"
"    ],\n"
"    \"group\": \"build\"\n"
"}\n"
"```"

